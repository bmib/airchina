package com.ass.component.tree;
/**
 * 树对象描述类,用于初始化不同的树形结构
 * @author Mr.liuyong
 */
public class TreeDefinition {
	//树-表名
	private String tableName="tableName";
	//树-主键列名
	private String tableUUID="tableUUID";
	//树-父级节点列名
	private String tablePID="tablePID";
	//树-层级属性列名
	private String tableLevelcode="tableLevelcode";
	//树-节点排序号列名
	private String tableSortNum="tableSortNum";
	/**
	 * 返回变量tableName
	 * @return the tableName
	 */
	public String getTableName() {
		return tableName;
	}
	/**
	 * 设置变量tableName
	 * @param tableName the tableName to set
	 */
	public void setTableName(String tableName) {
		this.tableName = tableName;
	}
	/**
	 * 返回变量tableUUID
	 * @return the tableUUID
	 */
	public String getTableUUID() {
		return tableUUID;
	}
	/**
	 * 设置变量tableUUID
	 * @param tableUUID the tableUUID to set
	 */
	public void setTableUUID(String tableUUID) {
		this.tableUUID = tableUUID;
	}
	/**
	 * 返回变量tablePID
	 * @return the tablePID
	 */
	public String getTablePID() {
		return tablePID;
	}
	/**
	 * 设置变量tablePID
	 * @param tablePID the tablePID to set
	 */
	public void setTablePID(String tablePID) {
		this.tablePID = tablePID;
	}
	/**
	 * 返回变量tableLevelcode
	 * @return the tableLevelcode
	 */
	public String getTableLevelcode() {
		return tableLevelcode;
	}
	/**
	 * 设置变量tableLevelcode
	 * @param tableLevelcode the tableLevelcode to set
	 */
	public void setTableLevelcode(String tableLevelcode) {
		this.tableLevelcode = tableLevelcode;
	}
	/**
	 * 返回变量tableSortNum
	 * @return the tableSortNum
	 */
	public String getTableSortNum() {
		return tableSortNum;
	}
	/**
	 * 设置变量tableSortNum
	 * @param tableSortNum the tableSortNum to set
	 */
	public void setTableSortNum(String tableSortNum) {
		this.tableSortNum = tableSortNum;
	}
}
